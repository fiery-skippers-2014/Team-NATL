<h1>NATL Survey</h1>



<% if current_user && @surveys.count > 0 %>
  <ul>
    <% @surveys.each do |survey| %>
    <li><a href="/survey/<%= survey.id %>"><%= survey.title %></a> by <%= survey.user.username %></li>
    <% end %>
<% elsif !current_user%>
    <%= erb :_login %>
<% else  %>


  </ul>
  <h2>No surveys yet!</h2>

<% end %>